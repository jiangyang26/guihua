/* pages/profile/history-fast/history-fast.wxss */
.history_fast{
  padding: 0rpx 30rpx 112rpx;
  box-sizing: border-box;
  overflow: scroll;
  background-color: #FAFCFF;
}
.h_search{
  margin-top: 14rpx;
  height: 64rpx;
  background: #EEEEEE;
  border-radius: 64rpx;
  opacity: 1;
  padding-left: 80rpx;
  box-sizing: border-box;

  font-size: 13px;
  font-weight: 400;
  color: #999999;
  line-height: 15px;
}
.search_box{
  position: relative;
  margin-bottom: 14rpx;
}
.search_box>image{
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  top: 16rpx;
  left: 28rpx;
}

.fast_list{
  height: 256rpx;
  background: #FFFFFF;
  box-shadow: 0px 8rpx 24rpx 2rpx rgba(0, 0, 0, 0.05);
  border-radius: 16rpx;
  margin-top: 32rpx;
  padding: 0rpx 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-flow: column;
}
.fast_head{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 32rpx;
}
.fast_head image{
  width: 24rpx;
  height: 24rpx;
}
.fast_head>text{
  font-size: 32rpx;
  font-weight: bold;
  color: #000000;
  line-height: 38rpx;
}
.fast_head>view{
  font-size: 0;
  display: flex;
  align-items: center;
}
.fast_head>view>text{
  font-size: 26rpx;
  font-weight: 400;
  color: #5350D5;
  line-height: 30rpx;
  margin-right: 8rpx;
}
.lines{
  height: 1px;
  background: #E5E5E5;
  border-radius: 0px 0px 0px 0px;
  opacity: 1;
  margin-top: 16rpx;
}
.fast_date{
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  line-height: 32rpx;
  margin-top: 34rpx;
}
.fast_budget{
  font-size: 28rpx;
  font-weight: 400;
  color: #000000;
  line-height: 32rpx;
  margin-top: 10rpx;
}