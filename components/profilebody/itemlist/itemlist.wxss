/* components/profilebody/itemlist/itemlist.wxss */
.item_list{
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  flex: 1;
  width: 152rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: #333333;
}
.item_list image{
  width: 152rpx;
  height: 136rpx;
  font-size: 0;
  border-radius: 14rpx;
  box-shadow: 0px 0px 8px 1px rgba(0, 0, 0, 0.1);
  margin-bottom: 40rpx;
}