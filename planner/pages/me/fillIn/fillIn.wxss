page {
  background: #FAFCFF;
  padding: 0 30rpx 112rpx;
  box-sizing: border-box;
}
.box {
  margin-top: 14rpx;
  padding: 30rpx 40rpx 38rpx 30rpx;
  box-shadow: 0 0 24rpx 2rpx rgba(0, 0, 0, 0.1);
  border-radius: 16rpx;
}
.box .userInfo_box {
  margin-bottom: 46rpx;
  display: flex;
  align-items: center;
}
.box .userInfo_box .userInfo_img {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
}
.box .userInfo_box .userInfo_code {
  margin-left: 34rpx;
  font-size: 20rpx;
}
.box > picker {
  margin-top: 36rpx;
}
.box .line {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 36rpx;
}
.box .line .right_box {
  display: flex;
}
.box .line .right_box text {
  flex: 1;
  margin-right: 20rpx;
  overflow: hidden;
  white-space: pre;
  text-overflow: ellipsis;
  text-align: right;
}
.box .line:first-child {
  margin-top: 0;
}
.box .line > .left,
.box .line > .right {
  font-size: 28rpx;
  width: 50%;
}
.box .line > .right {
  display: flex;
  justify-content: flex-end;
}
.box .line .right_text {
  float: 1;
}
.box .line .right_text text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.box .line>picker {
  width: 50%;
}
.box .line picker .left,
.box .line picker .right {
  font-size: 28rpx;
}
.box .line picker .right {
  display: flex;
  justify-content: flex-end;
}
.box .line .write {
  width: 110rpx;
  height: 48rpx;
  border: 1px solid #5350D5;
  border-radius: 8rpx;
  color: #5350D5;
  font-size: 24rpx;
}
.box .line .radio_box {
  display: flex;
}
.box .line .radio_box .radio {
  margin-left: 32rpx;
  display: flex;
  align-items: center;
}
.box .line .radio_box .radio > view {
  margin-right: 10rpx;
  width: 34rpx;
  height: 34rpx;
  border: 1px solid #D8D8D8;
  border-radius: 50%;
  box-sizing: border-box;
}
.box .line .radio_box .radio image {
  margin-right: 10rpx;
  width: 34rpx;
  height: 34rpx;
}
.box .select {
  margin-top: 40rpx;
  font-size: 28rpx;
}
.box .select .tag_box {
  display: flex;
}
.box .select .add_box {
  position: relative;
}
.box .select .add_box .van-icon {
  position: absolute;
  top: 4rpx;
  right: 4rpx;
}
.box .select .add {
  /* margin-top: 20rpx; */
  width: 24%;
  height: 64rpx;
  background-color: #F5F5F5;
  border-radius: 8rpx;
  border: 1px solid #eeeeee;
  padding: 0rpx 10rpx;
  box-sizing: border-box;
}
.box .select .add text {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.box .select .file {
  width: 128rpx;
  height: 128rpx;
}
.box .grow_box {
  margin-top: 24rpx;
  margin-right: -16rpx;
  display: flex;
  flex-wrap: wrap;
}
.box .grow_box .grow {
  padding: 0 20rpx;
  text-align: center;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  width: 194rpx;
  height: 118rpx;
  margin-right: 16rpx;
  font-size: 26rpx;
  border-radius: 8rpx;
}
.box .grow_box .ec {
  background-color: #ECF2FB;
}
.box .grow_box .ee {
  background-color: #EEEEEE;
}
.box .grow_box textarea {
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  width: 630rpx;
  height: 184rpx;
  background-color: #F5F5F5;
  border-radius: 8rpx;
  border: 1px solid #EEEEEE;
  font-size: 26rpx;
}
.title {
  margin: 48rpx 0 30rpx;
  font-size: 36rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.title .btn {
  width: 240rpx;
  height: 64rpx;
  font-size: 28rpx;
  color: #5357EB;
  border-radius: 88rpx;
  border: 1px solid #5350D5;
}
.table {
  font-size: 26rpx;
  border-radius: 16rpx;
}
.table .table_r {
  height: 62rpx;
  display: flex;
  align-items: center;
  border: 1px solid #dddddd;
  box-sizing: border-box;
  border-top: none;
}
.table .table_r:last-child {
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
}
.table .table_r .table_th {
  width: 130rpx;
  height: 100%;
  border-right: 1px solid #dddddd;
  box-sizing: border-box;
}
.table .table_r .table_td {
  flex: 1;
  display: flex;
  align-items: center;
  height: 100%;
  margin-left: 24rpx;
}
.table .table_r .grey {
  color: #666;
}
.table .table_tit {
  height: 72rpx;
  color: #fff;
  background-color: #5350D5;
  border-radius: 16rpx 16rpx 0 0;
  border: none;
}
.table .table_tlt {
  display: flex;
  align-items: center;
  height: 88rpx;
  background-color: #5350D5;
  color: #fff;
  border-radius: 16rpx 16rpx 0 0;
}
.table .table_tlt .table_th {
  font-size: 22rpx;
  border-right: 1px solid #5350D5;
}
.table .table_row {
  height: 62rpx;
  display: flex;
  align-items: center;
  border: 1px solid #dddddd;
  border-top: none;
}
.table .table_row:last-child {
  border-bottom-left-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
}
.table .table_row > view {
  height: 100%;
  font-size: 16rpx;
  border-right: 1px solid #dddddd;
}
.table .table_row > view:last-child {
  border-right: none;
}
textarea {
  padding: 30rpx;
  box-sizing: border-box;
  width: 100%;
  height: 240rpx;
  border: 1px solid #DDDDDD;
  border-radius: 16rpx;
  font-size: 30rpx;
}
button {
  margin: 64rpx 0 124rpx;
  width: 100%;
  height: 96rpx;
  background-color: #5350D5;
  color: #fff;
  font-size: 36rpx;
  border-radius: 88rpx;
}
.keyboard {
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.4);
  z-index: 10;
}
.keyboard .input_box {
  width: 100%;
  height: 86rpx;
  position: fixed;
  bottom: 100px;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.keyboard .input_box input {
  padding: 0 24rpx;
  height: 100%;
  flex: 1;
}
.keyboard .input_box > view {
  width: 140rpx;
  height: 100%;
  background-color: #5350D5;
  color: #fff;
}


/* ************************ */
.plac_{
  font-size: 26rpx;
  font-weight: 500;
  color: #999999;
  line-height: 30rpx;
}
.choose_plan{
  margin-top: 30rpx;
}
.plan_top{
  display: flex;
  height: 340rpx;
  justify-content: space-between;
  box-sizing: border-box;
}
.plan_bottom{
  height: 92px;
  margin-top: 30rpx;
}
.plan_bottom>textarea{
  width: 100%;
  height: 100%;
  background: #F5F5F5;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-radius: 4px 4px 4px 4px;
  border: none;
  line-height: 30rpx;
  font-size: 26rpx;
}
.left_>view, .center_>view, .right_>view{
  padding: 20rpx;
  box-sizing: border-box;
}
.left_ view{
  width: 190rpx;
}
.center_ view{
  width: 208rpx;
}
.right_{
  width: 192rpx;
}
.left_, .center_, .right_{
  display: flex;
  flex-flow: column;
}
.left_ view, .center_ view, .right_ view{
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  border-radius: 8rpx;
}
.left_view1{
  height: 118rpx;
  background: #ECF2FB;
}
.left_view2{
  height: 88rpx;
  background: #FDEEEA;
}
.left_view3{
  height: 72rpx;
  background: #F5F5F5;
}
.left_, .center_, .right_{
  justify-content: space-between;
}
.center_view1{
  height: 88rpx;
  background: #F5F5F5;
}
.center_view2{
  height: 118rpx;
  background: #ECF2FB;
}
.center_view3{
  height: 72rpx;
  background: #F5F5F5;
}
.right_view1{
  height: 72rpx;
  background: #F5F5F5;
}
.right_view2{
  height: 136rpx;
  background: #ECF2FB;
}
.right_view3{
  height: 74rpx;
}
.mark_t{
  font-size: 26rpx;
  font-weight: 500;
  color: #000000;
}
.active{
  background: #5350D5;

}
.active text{
  font-size: 26rpx;
  font-weight: bold;
  color: #FFFFFF;
}
.territory_tag_box{
  width: 100%;
  height: 64rpx;
  display: flex;
  margin-top: 20rpx;
}
.item_box{
  width: 194rpx;
  height: 64rpx;
  background-color: #F5F5F5;
  border-radius: 8rpx;
  border: 1px solid #eeeeee;
  padding: 0rpx 26rpx;
  margin-right: 16rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}
.item_box>text{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.territory_tag_box>.item_box>van-icon{
  position: absolute;
  top: 4rpx;
  right: 4rpx;
}
.input_mark{
  font-size: 30rpx; 
  width: 100%; height: 70rpx;
  background-color: #EEEEEE;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.upload_box_flex{
  display: flex;
  margin-top: 20rpx;
}
.image_box{
  width: 128rpx;
  height: 128rpx;
}
.image_box>image{
  width: 128rpx;
  height: 128rpx;
}
.status_box{
  display: flex;
  font-size: 0;
  align-items: center;
}
.status_box image{
  width: 48rpx;
  height: 48rpx;
}
.status_box text{
  font-size: 32rpx;
  margin-left: 10rpx;
}